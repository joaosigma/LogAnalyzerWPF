<adonisControls:AdonisWindow x:Class="LogAnalyzerWPF.Windows.WindowCommandGeneric"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                             xmlns:localControls="clr-namespace:LogAnalyzerWPF.Controls"
                             xmlns:adonisUi="clr-namespace:AdonisUI;assembly=AdonisUI"
                             xmlns:adonisControls="clr-namespace:AdonisUI.Controls;assembly=AdonisUI"
                             mc:Ignorable="d"
                             Title="Generic command output" ShowInTaskbar="False" Width="500" Height="400" WindowStartupLocation="CenterOwner" IconVisibility="Collapsed">
    <Grid>
        <ScrollViewer>
            <localControls:SelectableTextBlock Margin="10" x:Name="mTextBlock" TextWrapping="Wrap" FontSize="13" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            </localControls:SelectableTextBlock>
        </ScrollViewer>
    </Grid>
</adonisControls:AdonisWindow>
